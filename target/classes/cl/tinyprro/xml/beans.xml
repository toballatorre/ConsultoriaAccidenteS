<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="persona" class="cl.tinyprro.beans.Persona">
		<property name="nombre" value="Tobal"></property>
		<property name="apellido" value="Guzmán"></property>
		<property name="edad" value="38"></property>
	</bean>
	
	<!-- BEAN CAPACITACION -->
	<bean id="capacitacion" class="cl.tinyprro.beans.Capacitacion">
		<property name="id" value="1" />
		<property name="idCliente" value="2" />
		<property name="tema" value="Derecho a Saber" />
		<property name="objetivos" value="Informar a sus colaboradores sobre los peligros y riesgos que están presentes en sus labores" />
		<property name="contenidos" value="* Introduccion *Riesgos *Desarrollo * Preguntas *Cierre" />
		<property name="recursos" value="Sala, Computador, Notebook" />
		<property name="fecha" value="01/05/20" />
		<property name="idUsuarioPro" value="7" />
	</bean>
	
</beans>
